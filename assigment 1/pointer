#include <bits/stdc++.h>

void update(int *a,int *b) {
    // Complete this function    
    int k,l;
    k=*a+*b;
    
    l=abs(*a-*b);
    *a=k;
    *b=l;
    
    
}

int main() {
    int a, b;
    int *pa = &a, *pb = &b;
    
    scanf("%d %d", &a, &b);
    update(pa, pb);
    printf("%d\n%d", a, b);

    return 0;
}
