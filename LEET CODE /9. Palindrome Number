class Solution {
public:
    bool isPalindrome(int x) {
        long y=0,rem=0,n=0;
        n=x;
        if(x<0)
        {return false;}
        else 
        {
       while (n!=0)
      { 
            
            y=y * 10 + n % 10;
            n=n/10;

        }
        if (x==y)
        return true;
        else
        return false;
        }
    }
};
